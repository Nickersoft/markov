    "url": "https://github.com/Nickersoft/markov/issues"
  },
  "homepage": "https://github.com/Nickersoft/markov#readme",
  "devDependencies": {
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "xmlhttprequest": "^1.8.0"
